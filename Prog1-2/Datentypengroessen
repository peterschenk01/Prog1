#include <stdio.h>

int main(){
	printf("Size of char: %Iu\n", sizeof(char));
	printf("Size of short: %Iu\n", sizeof(short));
	printf("Size of int: %Iu\n", sizeof(int));
	printf("Size of long: %Iu\n", sizeof(long));
	printf("Size of float: %Iu\n", sizeof(float));
	printf("Size of double: %Iu\n", sizeof(double));
	printf("Size of long double: %Iu\n", sizeof(long double));
	printf("Size of signed char: %Iu\n", sizeof(signed char));
	printf("Size of signed short: %Iu\n", sizeof(signed short));
	printf("Size of signed int: %Iu\n", sizeof(signed int));
	printf("Size of signed long: %Iu\n", sizeof(signed long));
	printf("Size of unsigned char: %Iu\n", sizeof(unsigned char));
	printf("Size of unsigned short: %Iu\n", sizeof(unsigned short));
	printf("Size of unsigned int: %Iu\n", sizeof(unsigned int));
	printf("Size of unsigned long: %Iu\n", sizeof(unsigned long));
	return 0;
}
